public class App 
{
    public static void main( String[] args )
    {
	Scanner sc = new Scanner(System.in);
	String s;
	PhoneBook p = new PhoneBook("", new Vector());

        System.out.println( "Enter data " );	
	s = sc.nextLine();
	p.parseAndSet(s);
	p.printPhone();
    }
}
