package ru.apache_maven;


import java.util.Scanner;
import java.util.ArrayList;
import java.util.Map;


class PhoneBookRecord
{
	private Map<String, ArrayList> m_rec = new HashMap<String, Arraylist>();
	
	PhoneBookRecord(String name, ArrayList phones)
	{
		m_rec.put(name, phones);
		
	}
	public String getName()
	{
		return m_rec.getKey;	
	}
	public ArrayList getPhones()
	{
		return m_rec.getValue();	
	}
	public void parseAndSet(String data)
	{
		String[] tmp = data.split("\\+");
		String name = tmp[0];
		ArrayList phones = new ArrayList();
		for (int i = 1; i <= tmp.length-1; ++i)
		{
		    phones.add("+"+tmp[i]);
		}
		m_rec.put(name, phones);
		
	}
	public void printPhone(){
		System.out.println(m_rec.getKey() + " " + m_rec.getValue().toString()); 
	}
}
